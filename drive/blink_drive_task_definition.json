[
  {
    "name": "blink-drive-1",
    "image": "jerridan/blink-drive",
    "memory": 512,
    "cpu": 10,
    "privileged": true,
    "portMappings": [
      {
        "containerPort": 443,
        "hostPort": 443,
        "protocol": "tcp"
      },
      {
        "containerPort": 1194,
        "hostPort": 1194,
        "protocol": "udp"
      }
    ],
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group": "blink-drive-log-group",
        "awslogs-region": "ca-central-1",
        "awslogs-stream-prefix": "blink-drive"
      }
    }
  }
]